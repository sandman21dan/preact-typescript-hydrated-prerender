{
  "name": "preact-typescript-minimal",
  "version": "1.0.0",
  "description": "Minimal preact typescript starter",
  "main": "index.js",
  "scripts": {
    "build:info": "npm run build -- --detailed-report",
    "build": "npm run clean:dist && parcel build src/index.html --public-url ./",
    "clean:cache": "rimraf .cache",
    "clean:dist": "rimraf dist",
    "deploy": "gh-pages -d ./dist",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "parcel src/index.html",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "author": "Daniel Perez",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "preact": "^8.4.2",
    "preact-redux": "^2.0.3",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/redux-actions": "^2.3.2",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-prerender": "^1.2.0",
    "rimraf": "^2.6.3",
    "sass": "^1.17.2",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "typescript": "^3.3.3"
  }
}
