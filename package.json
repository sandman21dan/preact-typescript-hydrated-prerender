{
  "name": "preact-typescript-minimal",
  "version": "1.0.0",
  "description": "Minimal preact typescript starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean:dist && parcel build src/index.html --public-url ./",
    "clean": "npm run clean:cache && npm run clean:dist",
    "clean:cache": "rimraf .cache",
    "clean:dist": "rimraf dist",
    "deploy": "gh-pages -d ./dist",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "parcel src/index.html",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "author": "Daniel Perez",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "preact": "^8.4.2",
    "preact-redux": "^2.0.3",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/redux-actions": "^2.3.2",
    "axios-mock-adapter": "^1.17.0",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-prerender": "^1.4.1",
    "rimraf": "^3.0.0",
    "sass": "^1.23.7",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
